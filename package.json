  "bugs": {
    "url": "https://github.com/jriccrussel/jobify/issues"
  },
  "homepage": "https://github.com/jriccrussel/jobify#readme",
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  }
}
